{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "qtmusic",
	"version": "0.1.0",
	"identifier": "top.rumble.qtmusic",
	"build": {
		"beforeDevCommand": "pnpm vdev",
		"devUrl": "http://localhost:1420",
		"beforeBuildCommand": "pnpm vbuild",
		"frontendDist": "../dist"
	},
	"app": {
		"windows": [
			{
				"label": "main",
				"title": "QTMusic",
				"width": 1050,
				"height": 590,
				"minWidth": 1050,
				"minHeight": 590,
				"decorations": false,
				"transparent": true
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		],
		"resources": {
			"config.json": "config.json",
			"deviceid.txt": "deviceid.txt"
		}
	}
}
